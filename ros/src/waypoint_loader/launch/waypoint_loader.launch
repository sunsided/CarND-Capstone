<?xml version="1.0"?>
<launch>
    <node pkg="waypoint_loader" type="waypoint_loader.py" name="waypoint_loader">
        <!-- 
        Simulator "Highway" track data: $(find styx)../../../data/wp_yaw_const.csv
        Test lot data:                  $(find styx)../../../data/churchlot_with_cars.csv
        To download test data, get https://s3-us-west-1.amazonaws.com/udacity-selfdrivingcar/files/reference.bag.zip
        -->
        <param name="path" value="$(find styx)../../../data/wp_yaw_const.csv" />
        <param name="velocity" value="40" />
    </node>
</launch>
